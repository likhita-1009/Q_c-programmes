//without arrgumennt and with return value
#include<stdio.h>
int prime()
{
	int a,res=0;
	scanf("%d",&a);
	int i;
	for(i=2;i<=a/2;i++)
	{
		if(a%i!=0)
			break;
		else
			return 1;
	}
	return 0;
}
int main()
{
	int res;
	res=prime();
	if(res==0)
		printf("prime");
	else
		printf("\nis not prime");
}
