//with arguments and no return values
#include<stdio.h>
int str(int a);
int pos(int b);
int main()
{
	int a,b,c=0,i,j;
	scanf("%d",&a);
	str(a);
	printf("\n");
	pos(a);
}
int str(int a)
{
	int t=a;
    int i,j,b,c=0;
	for(i=a;a!=0;a=a/10){
		b=a%10;
		for(j=b-1;j>0;j--){
			b=b*j;
		}
		c=c+b; 
    }
     if(t==c){
     	printf("it is strong number");
	 }else {
	 	printf("it is not strong number");
	 }
}
int pos(int b)
{
	if(b>=0){
		printf("positive");
	}else{
		printf("negative");
	}
}
