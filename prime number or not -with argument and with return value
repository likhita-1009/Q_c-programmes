//with arrgument and with return value
#include<stdio.h>
int prime(int n)
{
	int i;
	for(i=2;i<=n/2;i++)
	{
		if(n%i!=0)
			break;
		else
			return 1;
	}
	return 0;
}
int main()
{
	int a,res=0;
	scanf("%d",&a);
	res=prime(a);
	if(res==0)
		printf("%d is prime",a);
	else
		printf("\n%d is not prime",a);
}
